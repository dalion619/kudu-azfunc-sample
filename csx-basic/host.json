{
    "version": "2.0",
    "extensions": {
        "http": {
            "routePrefix": "func",
            "maxConcurrentRequests": 5,
            "maxOutstandingRequests": 30
        }
    },
    "watchDirectories": [
        "TestHttp"
    ],
    "logging": {
        "fileLoggingMode": "always",
        "logLevel": {
            "default": "Debug"
        }
    },
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[4.*, 5.0.0)"
    }
}